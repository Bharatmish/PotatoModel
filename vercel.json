{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/main-tf-serving.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/main",
        "dest": "api/main.py"
      },
      {
        "src": "/api/main-tf-serving",
        "dest": "api/main-tf-serving.py"
      }
    ]
  }
  